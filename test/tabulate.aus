val op>= = builtin ">="

exception Size

fun rev l =
    let
	fun rev( nil,  ys) = ys
	  | rev(x::xs, ys) = rev(xs, x::ys)
    in
	rev(l,nil)
    end

fun tabulate(n, f) =
    let
	fun tabulate(i,l) =
	    if i = n then rev l
	    else tabulate(i+1, f i :: l)
    in
	if n >= 0 then tabulate(n, nil)
	else raise Size
    end
