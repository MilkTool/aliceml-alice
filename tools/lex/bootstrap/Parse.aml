import structure Jacke from "jacke.jacke"
import structure HoseLexer from "hose.hose"
import signature PARSE from "PARSE-sig"

structure Parse :> PARSE =
    struct

	(* parse : string -> AbsSyn.lex list, returns the abstract datatree
	 *)
	fun parse str = Jacke.eval (HoseLexer.lexString str)
	    
    end
