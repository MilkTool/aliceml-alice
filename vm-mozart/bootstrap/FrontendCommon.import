(* -*- sml -*- *)

import

###frontend-common/FIXITY.sig
structure Fixity: FIXITY

###frontend-common/INF.sig
structure Inf: INF

###frontend-common/ABSTRACT_GRAMMAR.sig
structure AbstractGrammar: ABSTRACT_GRAMMAR
  where type lab_info	= Source.region
  where type id_info	= Source.region
  where type longid_info	= Source.region
  where type exp_info	= Source.region
  where type pat_info	= Source.region
  where type 'a row_info	= Source.region
  where type 'a field_info	= Source.region
  where type match_info	= Source.region
  where type typ_info	= Source.region
  where type con_info	= Source.region
  where type mod_info	= Source.region
  where type inf_info	= Source.region
  where type dec_info	= Source.region
  where type spec_info	= Source.region
  where type comp_info	= Source.region
  where type imp_info	= Source.region

structure TypedGrammar: ABSTRACT_GRAMMAR
  where type lab_info	= { region: Source.region }
  where type id_info	= { region: Source.region }
  where type longid_info	= { region: Source.region }
  where type exp_info	= { region: Source.region, typ: Type.t }
  where type pat_info	= { region: Source.region, typ: Type.t }
  where type 'a row_info	= { region: Source.region }
  where type 'a field_info	= { region: Source.region }
  where type match_info	= { region: Source.region }
  where type typ_info	= { region: Source.region, typ: Type.t }
  where type con_info	= { region: Source.region, typ: Type.t }
  where type mod_info	= { region: Source.region, inf: Inf.t }
  where type inf_info	= { region: Source.region, inf: Inf.t }
  where type dec_info	= { region: Source.region }
  where type spec_info	= { region: Source.region }
  where type comp_info	= { region: Source.region }
  where type imp_info	= { region: Source.region }

###frontend-common/ENV.sig
structure Env: ENV

###frontend-common/ENV0.sig
structure Env0: ENV0

###frontend-common/ELABORATION_PHASE.sig
structure ElaborationPhase: ELABORATION_PHASE

###frontend-common/TRANSLATION_PHASE.sig
structure TranslationPhase: TRANSLATION_PHASE

