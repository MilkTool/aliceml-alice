import structure UnsafeMinimizer from "UnsafeMinimizer"
import signature MINIMIZER       from "MINIMIZER-sig"

structure Minimizer : MINIMIZER =
  struct
    fun minimize a = let
        val b = UnsafeMinimizer.newPartition()
        val _ = UnsafeMinimizer.loadGraph(b, a)
    in
        UnsafeMinimizer.minimize b
    end
  end
