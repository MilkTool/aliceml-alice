import structure Url from "../utility/Url"

signature URL_MAP =
    sig
	type 'a t

	datatype 'a lookup_result =
	    EXISTING of 'a
	  | NEW of 'a Promise.promise

	val new: unit -> 'a t

	val lookup: 'a t * Url.t -> 'a option
	val lookupNew: 'a t * Url.t -> 'a lookup_result
    end
