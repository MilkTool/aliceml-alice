import structure UnsafeSocket from "UnsafeSocket"
import signature SOCKET       from "SOCKET-sig"

structure Socket :> SOCKET =
    struct
	type t
	type vector = string
	type elem = char

	type host = string
	type port = int

	open UnsafeSocket
    end
