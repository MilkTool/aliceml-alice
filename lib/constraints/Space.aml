import structure UnsafeSpace from "UnsafeSpace"
import signature SPACE from "SPACE-sig"
	
structure Space :> SPACE =
    struct
	type 'a space
	    
	datatype status =
	    FAILED
	  | SUCCEEDED
	  | ALTERNATIVES of int
	    
	datatype choice =
	    SINGLE of int
	  | RANGE of int * int
	    
	open UnsafeSpace
    end
