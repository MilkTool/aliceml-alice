signature FMT =
sig
	datatype format =
		S of string
	|	I of int
	|	R of real
	|	B of bool
	|	HEX of int
	|	OCT of int
	|	BIN of int
	|	SPC
	|	NL 						(* newline *)
	|	TAB						(* tabulator *)

	val fmt: TextIO.outstream -> format list -> unit
end


