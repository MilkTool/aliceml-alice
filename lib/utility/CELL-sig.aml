import signature IMP_MAP from "IMP_MAP-sig"

signature CELL =
sig
    eqtype 'a cell
    type 'a t = 'a cell

    val new :		'a -> 'a cell
    val content :	'a cell -> 'a
    val replace :	'a cell * 'a -> unit
    val equal :		'a cell * 'a cell -> bool

    functor MkMap(type t) : IMP_MAP where type key = t cell
end
