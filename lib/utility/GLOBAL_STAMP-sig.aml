import signature STAMP from "STAMP-sig"


signature GLOBAL_STAMP =
sig
    (*include STAMP*)

    eqtype stamp
    type t = stamp

    val new :		unit   -> stamp
    val fromString :	string -> stamp
    val toString :	stamp  -> string

    val hash :		stamp -> int
end
