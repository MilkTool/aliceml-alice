import signature SPACE        from "SPACE-sig"
import structure UnsafeGecode from "UnsafeGecode"

structure Space :> SPACE =
struct
    type 'problem space
    type 'problem fdvar
    type 'problem boolvar

    datatype status = BRANCH | FAILED | SOLVED

    open UnsafeGecode
end
