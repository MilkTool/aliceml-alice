import structure PrettyPrint from "../misc/PrettyPrint"
import structure Inf         from "Inf"

signature PP_INF =
sig
    type doc  = PrettyPrint.doc
    type inf  = Inf.inf
    type sign = Inf.sign

    val ppInf :	inf -> doc
    val ppSig : sign -> doc
end
