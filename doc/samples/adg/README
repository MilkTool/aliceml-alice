	alicerun Main ../snake/client/ClientMain.aml
	alicerun Main ../snake/client/ClientMain.aml snake_out 
	alicerun Main ../snake/client/ClientMain.aml -no <regex>
	alicerun Main ../snake/client/ClientMain.aml -only <regex>


